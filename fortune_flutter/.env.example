# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key

# Edge Functions Configuration
EDGE_FUNCTIONS_URL=https://your-project.supabase.co/functions/v1

# API Configuration (Legacy - will be removed)
API_BASE_URL=http://localhost:3000

# Google OAuth Configuration
GOOGLE_WEB_CLIENT_ID=your-google-web-client-id
GOOGLE_IOS_CLIENT_ID=your-google-ios-client-id

# Environment
ENVIRONMENT=development

# Feature Flags
USE_EDGE_FUNCTIONS=false
EDGE_FUNCTIONS_ROLLOUT_PERCENTAGE=10

# In-App Purchase Product IDs
IAP_PRODUCT_TOKEN_SMALL=com.fortune.tokens.small
IAP_PRODUCT_TOKEN_MEDIUM=com.fortune.tokens.medium
IAP_PRODUCT_TOKEN_LARGE=com.fortune.tokens.large
IAP_PRODUCT_TOKEN_MEGA=com.fortune.tokens.mega

# Monitoring
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# AdMob Configuration (Optional)
ADMOB_APP_ID_IOS=ca-app-pub-your-app-id
ADMOB_APP_ID_ANDROID=ca-app-pub-your-app-id
ADMOB_BANNER_ID_IOS=ca-app-pub-your-banner-id
ADMOB_BANNER_ID_ANDROID=ca-app-pub-your-banner-id

# Naver Login Configuration
NAVER_CLIENT_ID=your-naver-client-id
NAVER_CLIENT_SECRET=your-naver-client-secret
NAVER_URL_SCHEME=your-url-scheme