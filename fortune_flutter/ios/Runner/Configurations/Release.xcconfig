#include "Pods/Target Support Files/Pods-Runner/Pods-Runner.release.xcconfig"
#include "Flutter/Release.xcconfig"

// Production Configuration
PRODUCT_BUNDLE_IDENTIFIER = com.fortune.fortune_flutter
PRODUCT_NAME = Fortune
DEVELOPMENT_TEAM = YOUR_TEAM_ID

// Code Signing
CODE_SIGN_STYLE = Manual
CODE_SIGN_IDENTITY = iPhone Distribution
PROVISIONING_PROFILE_SPECIFIER = fortune-release

// Build Settings
ENABLE_BITCODE = NO
GCC_OPTIMIZATION_LEVEL = s
SWIFT_OPTIMIZATION_LEVEL = -O

// Swift Flags
OTHER_SWIFT_FLAGS = $(inherited) -D RELEASE

// Deployment Target
IPHONEOS_DEPLOYMENT_TARGET = 12.0

// App Transport Security
// Ensure all network calls use HTTPS in production
ENABLE_NS_ASSERTIONS = NO