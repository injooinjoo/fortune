# Example environment variables for the fortune Next.js project.
# Copy this file to `.env.local` and provide your own values.

# Firebase configuration
NEXT_PUBLIC_FIREBASE_API_KEY=            # Firebase API key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=        # Firebase authentication domain
NEXT_PUBLIC_FIREBASE_PROJECT_ID=         # Firebase project ID
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=     # Firebase storage bucket name
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=# Firebase Cloud Messaging sender ID
NEXT_PUBLIC_FIREBASE_APP_ID=             # Firebase application ID
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=     # Firebase analytics measurement ID

# Google Genkit AI configuration
GOOGLE_GENAI_API_KEY=                    # API key for Google Genkit
