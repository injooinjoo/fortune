# Server Configuration
NODE_ENV=development
PORT=3001
API_VERSION=v1

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:9002

# Supabase
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# OpenAI
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4-turbo-preview

# Google AI (Genkit)
GOOGLE_AI_API_KEY=your_google_ai_api_key

# Redis (Upstash)
UPSTASH_REDIS_REST_URL=your_upstash_redis_rest_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_rest_token
REDIS_URL=your_redis_url

# Stripe
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key

# TossPay
TOSS_SECRET_KEY=your_toss_secret_key
TOSS_CLIENT_KEY=your_toss_client_key

# In-App Purchase
APPLE_IAP_SHARED_SECRET=your_apple_iap_shared_secret
GOOGLE_SERVICE_ACCOUNT_KEY_PATH=path_to_google_service_account_key.json

# Security
JWT_SECRET=your_jwt_secret
INTERNAL_API_KEY=your_internal_api_key
CRON_SECRET=your_cron_secret

# Database
DATABASE_URL=your_database_url

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# File Upload
MAX_FILE_SIZE=5242880
ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp

# App URLs
FRONTEND_URL=http://localhost:3000
APP_URL=https://fortune-app.com
API_URL=https://api.fortune-app.com