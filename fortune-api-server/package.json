{
  "name": "fortune-api-server",
  "version": "1.0.0",
  "description": "Fortune App API Server - Standalone Express server for Fortune services",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc || true",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "deploy": "./scripts/deploy.sh production",
    "deploy:staging": "./scripts/deploy.sh staging",
    "health-check": "./scripts/health-check.sh",
    "setup-env": "./scripts/setup-env.sh",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["fortune", "api", "express", "typescript"],
  "author": "Fortune Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "rate-limit-redis": "^4.2.0",
    "@supabase/supabase-js": "^2.39.3",
    "openai": "^4.47.1",
    "@genkit-ai/ai": "^1.8.0",
    "@genkit-ai/googleai": "^1.8.0",
    "@upstash/redis": "^1.28.0",
    "ioredis": "^5.3.2",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "dayjs": "^1.11.10",
    "stripe": "^14.10.0",
    "googleapis": "^128.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/node": "^20.10.6",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "@typescript-eslint/parser": "^6.17.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^6.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}