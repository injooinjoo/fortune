# Supabase 설정
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_JWT_SECRET=your-jwt-secret

# 개발 환경 설정
NODE_ENV=development

# Google Genkit AI 설정
GOOGLE_GENAI_API_KEY=your-google-genai-api-key

# Google OAuth 설정
GOOGLE_WEB_CLIENT_ID=your-google-web-client-id
GOOGLE_IOS_CLIENT_ID=your-google-ios-client-id
GOOGLE_ANDROID_CLIENT_ID=your-google-android-client-id

# OpenAI 설정
OPENAI_API_KEY=sk-proj-your-openai-api-key

# Google AdSense 설정
NEXT_PUBLIC_ADSENSE_CLIENT_ID=ca-pub-your-adsense-id
NEXT_PUBLIC_ADSENSE_SLOT_ID=your-slot-id
NEXT_PUBLIC_ADSENSE_DISPLAY_SLOT=your-display-slot

# 보안 키 설정 (생성 방법: openssl rand -hex 32)
INTERNAL_API_KEY=generate-random-hex-string
CRON_SECRET=generate-random-hex-string

# Stripe Payment 설정
STRIPE_SECRET_KEY=sk_test_your-stripe-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret
STRIPE_PREMIUM_MONTHLY_PRICE_ID=price_your-price-id
STRIPE_PREMIUM_YEARLY_PRICE_ID=price_your-price-id
STRIPE_TOKENS_SMALL_PRICE_ID=price_your-price-id
STRIPE_TOKENS_MEDIUM_PRICE_ID=price_your-price-id
STRIPE_TOKENS_LARGE_PRICE_ID=price_your-price-id

# Toss Payment 설정
TOSS_CLIENT_KEY=test_ck_your-client-key
TOSS_SECRET_KEY=test_sk_your-secret-key

# Upstash Redis 설정
UPSTASH_REDIS_REST_URL=https://your-redis-instance.upstash.io
UPSTASH_REDIS_REST_TOKEN=your-redis-token

# Sentry 에러 모니터링 설정
NEXT_PUBLIC_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
SENTRY_ORG=your-org-slug
SENTRY_PROJECT=your-project
SENTRY_AUTH_TOKEN=your_sentry_auth_token