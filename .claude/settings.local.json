{
  "permissions": {
    "allow": [
      "Bash(find:*)",
      "Bash(git checkout:*)",
      "Bash(flutter analyze:*)",
      "Bash(git branch:*)",
      "Bash(git restore:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(git merge:*)",
      "Read(//private/tmp/**)",
      "Bash(cat:*)",
      "Read(//tmp/**)",
      "Bash(dart run:*)",
      "Bash(flutter run:*)",
      "Bash(tee:*)",
      "Bash(./scripts/parse_ux_request.sh:*)",
      "Bash(./scripts/git_jira_commit.sh:*)"
    ],
    "deny": [],
    "ask": [],
    "defaultMode": "acceptEdits",
    "additionalDirectories": [
      "/private/tmp"
    ]
  },
  "enableAllProjectMcpServers": true,
  "enabledMcpjsonServers": [
    "figma-dev-mode-mcp-server"
  ],
  "mcpServers": {
    "framer": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-framer"
      ],
      "env": {}
    }
  }
}