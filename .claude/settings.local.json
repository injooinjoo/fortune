    "figma-dev-mode-mcp-server"
  ],
  "mcpServers": {
    "framer": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-framer"
      ],
      "env": {}
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@anthropic/mcp-server-playwright"
      ],
      "env": {
