{
  "permissions": {
    "allow": [
      "Bash(git rm:*)"
    ],
    "deny": [],
    "ask": [],
    "defaultMode": "acceptEdits",
    "additionalDirectories": [
      "/private/tmp"
    ]
  },
  "enableAllProjectMcpServers": true,
  "enabledMcpjsonServers": [
    "figma-dev-mode-mcp-server"
  ],
  "mcpServers": {
