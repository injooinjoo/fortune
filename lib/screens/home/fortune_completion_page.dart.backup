      'ê¸ˆì „': (baseValue * 0.98).clamp(0.0, 100.0),
      'ì§ì¥': (baseValue * 1.02).clamp(0.0, 100.0),
      'í•™ì—…': (baseValue * 0.92).clamp(0.0, 100.0),
      'ê±´ê°•': (baseValue * 0.97).clamp(0.0, 100.0),
    };
  }

