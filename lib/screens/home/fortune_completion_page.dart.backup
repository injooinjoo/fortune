  // í’€ì‚¬ì´ì¦ˆ ì¹´ë“œ ë¹Œë”
  Widget _buildFullSizeCard(
    BuildContext context,
    int index,
    int score,
    bool isDark,
    List<String> keywords,
    List<double> keywordWeights,
    List<int> hourlyScores,
    Map<String, dynamic> userStats,
  ) {
    final screenHeight = MediaQuery.of(context).size.height;
    final topPadding = MediaQuery.of(context).padding.top;
    final bottomPadding = MediaQuery.of(context).padding.bottom;
    final cardHeight = screenHeight - topPadding - bottomPadding - 80; // ë„¤ë¹„ê²Œì´ì…˜ ë°” ê³µê°„

    return Container(
      height: cardHeight,
      margin: const EdgeInsets.symmetric(horizontal: 16, vertical: 40),
      padding: const EdgeInsets.all(24),
      decoration: BoxDecoration(
        color: isDark ? TossDesignSystem.grayDark200 : TossDesignSystem.white,
        borderRadius: BorderRadius.circular(24),
        boxShadow: [
          BoxShadow(
            color: Colors.black.withOpacity(0.1),
            blurRadius: 20,
            offset: const Offset(0, 10),
          ),
        ],
      ),
      child: SingleChildScrollView(
        child: _buildCardContent(
          context,
          index,
          score,
          isDark,
          keywords,
          keywordWeights,
          hourlyScores,
          userStats,
        ),
      ),
    );
  }

